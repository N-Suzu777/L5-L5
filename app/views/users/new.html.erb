<%= form_tag users_create_path, method: :post, data: { turbo: false } do %>
    <%= label_tag :uid, 'ユーザーID' %>
    <%= text_field_tag :uid %>
    <%= label_tag :pass, 'パスワード' %>
    <%= password_field_tag :pass %>
    <%= submit_tag '登録' %>
<% end %>

<br>
<%= link_to "ユーザー一覧に戻る" ,:back %>
<br>
<%= link_to "トップに戻る" , root_path %>